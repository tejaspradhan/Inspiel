<div class="card">

  <div class="cr">
    <div class="c">
      <form action="/add_slot" method="POST">
        <div class="" style="align-items: center;"><br>
          <h3>Start Time</h3>
          <div class="s">
          <input type="number" name="starthr" placeholder="00" value="00" required>
          <input type="number" name="startmin" placeholder="00" value="00" required>
          <select class="" name="am">
            <option value="AM">AM</option>
            <option value="PM">PM</option>
          </select>
        </div>
        </div>
    <br>
        <div class="">
          <h3>End Time</h3>
          <div class="e">
          <input type="number" name="endhr" placeholder="00" value="00" required>
          <input type="number" name="endmin" placeholder="00" value="00" required>
          <select class="" name="pm">
            <option value="AM">AM</option>
            <option value="PM">PM</option>
          </select>
        </div>
        </div><br>
        <button class="bu"><span>Submit</span></button><br><br>
      </form>
