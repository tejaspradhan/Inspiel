<br>
<!-- Table starts-->
<div id="container" >
  <table id="table-1">
    <thead id="header-fixed">
        <tr style="align-items: center;"><th colspan="1" class="fth">Start Time</th><th class="sth">End Time</th></tr>
    </thead>
    <tbody >

  <% for (var slots of past) { %>
    <tr>
      <td><%= String(slots.startTime.hours).padStart(2,'0')%>:<%= String(slots.startTime.minutes).padStart(2,'0')%>  <%= slots.startTime.am%></td>
    <td><%= String(slots.endTime.hours).padStart(2,'0')%>:<%= String(slots.endTime.minutes).padStart(2,'0')%>  <%= slots.endTime.pm%></td>
  </tr>
    <% } %>


    </tbody>
  </table>
