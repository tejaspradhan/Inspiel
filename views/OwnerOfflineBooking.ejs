 <legend class="title">Select Date</legend>
 <%= court.name %> <%= court.sport %>
  <form action="/customer/court/<%= court.name %>/<%= court.sport %>" method="POST">
    <input type="text" name="name">Name</input>
              <div class="date-pick"  data-selected-id=-1>
                <div class="scroll-btn left">
                  <span class="scroll-img-wrap">
                    <svg "20px" height="30px">
                      <path fill="#ccc" d="M16.1,32l3.9-3.9L7.9,16L20,3.9L16.1,0L0,16L16.1,32z M17.1,3.9L5, 16l12.1,12.1l-1.1,1.1L2.9,16L16.1,2.9L17.1,3.9z" />
                    </svg>
                  </span>
                </div>
                <div class="date-wrapper">
                  <div class="date-scroll-container">
                    <% var future = new Date(); %>
                    <% futureNew= future.toLocaleDateString() %>


                    <label option-id="<%= i %>" class="timecell">
            <span class="date-item">
                <input type="radio" name="date" value="<%= futureNew %>" style="z-index: 1000" checked id= <%= future %> >
                <span class="fill-box"></span>
                <span class="time-day"><%= future.toString().slice(0, 3) %></span>
                <span class="time-date"><%= future.getDate() %></span>
                <span class="time-month"><%= future.toString().slice(4, 7) %></span>
                <span class="time-available">Slots available</span>
            </span>
            </label>


                    <% for (var i=0; i<=30; i++) {%>
                    <% date = new Date(future.setDate(future.getDate() + 1)) %>


                                    <label option-id="<%= i %>" class="timecell">
            <span class="date-item">
                <input type="radio" name="date" value="<%= date.toLocaleDateString() %>" style="z-index: 1000" checked id= <%= date %> >
                <span class="fill-box"></span>
                <span class="time-day"><%= date.toString().slice(0, 3) %></span>
                <span class="time-date"><%= date.getDate() %></span>
                <span class="time-month"><%= date.toString().slice(4, 7) %></span>
                <span class="time-available">Slots available</span>
            </span>
        </label>


                                    <%  console.log(date) %>
                    <% } %>
                    </div>
                  </div>
                  <div class="scroll-btn right">
                    <span class="scroll-img-wrap">
                      <svg width="20px" height="30px">
                        <path fill="#ccc" d="M3.9,0L0,3.9L12.1,16L0,28.1L3.9,32L20,16L3.9,0z M2.9,28.1L15, 16L2.9,3.9l1.1-1.1L17.1,16L3.9,29.1L2.9,28.1z"></path>
                      </svg>
                   </span>
                  </div>
                </div>
                <button>Submit</button>
</form>
